//-----------------------------------------------------------------------------
// Module: Decoder2
// Description: Evalúa la condición A0 XOR A1 y enciende un LED cuando aplica.
// Inputs: A0, A1
// Outputs: led_out (conectado a LED[0] de la FPGA)
//-----------------------------------------------------------------------------
module Decoder2 (
    input  logic A0,
    input  logic A1,
    output logic led_out
);

    assign led_out = A0 ^ A1;

endmodule
